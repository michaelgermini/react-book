{
  "name": "react-book-development",
  "version": "1.0.0",
  "type": "module",
  "description": "Complete development environment for React Book examples and projects",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md}\"",
    "type-check": "tsc --noEmit",
    "analyze": "npm run build && npx webpack-bundle-analyzer dist",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "setup": "npm install && npm run format && npm run lint:fix"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@reduxjs/toolkit": "^1.9.0",
    "@tanstack/react-query": "^4.29.0",
    "axios": "^1.3.0",
    "class-variance-authority": "^0.6.0",
    "clsx": "^1.2.0",
    "date-fns": "^2.29.0",
    "framer-motion": "^10.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.0",
    "react-hot-toast": "^2.4.0",
    "react-query": "^3.39.0",
    "react-redux": "^8.0.0",
    "react-router-dom": "^6.8.0",
    "recharts": "^2.5.0",
    "styled-components": "^5.3.0",
    "tailwindcss": "^3.2.0",
    "zustand": "^4.3.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@storybook/addon-essentials": "^6.5.0",
    "@storybook/addon-interactions": "^6.5.0",
    "@storybook/react": "^6.5.0",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^30.0.5",
    "cypress": "^12.0.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.4.0",
    "jest-environment-jsdom": "^29.4.0",
    "postcss": "^8.4.0",
    "prettier": "^2.8.0",
    "typescript": "^4.9.0",
    "vite": "^4.1.0",
    "webpack-bundle-analyzer": "^4.7.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "react",
    "typescript",
    "vite",
    "testing",
    "storybook",
    "cypress"
  ],
  "author": "React Book",
  "license": "MIT"
}
